{
  "buildCommand": "npm ci && npx tailwindcss -i ./global.css -o ./dist/global.css --minify && npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo start --web",
  "cleanUrls": true,
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/menus/:id*",
      "destination": "/index.html"
    }
  ]
}
